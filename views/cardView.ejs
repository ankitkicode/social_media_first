<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Bootstrap demo</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
  </head>
  <body  >
    <div class="container ">
      <div class="nav">
      
          
        <nav class=" navbar mt-2 border-bottom border-body" style="background-color: #e3f2fd; text-transform: capitalize; border-radius: 5px;">
          <div class="container-md">
              <a class="navbar-brand" href="/feed">feed</a>
              <a class="navbar-brand" href="/profile">profile</a>
              <a class="navbar-brand" href="/dashboard">dashboard</a>
              <a class="navbar-brand" href="/setting">setting</a>
              <a class="navbar-brand" href="/cardView">cardView</a>
            </div>
        </nav>
  </div>
        <div class="row">
          
            <% findAllUser.forEach(function(user){ %>
                <div class=" g-lg-4 col-4">
                <div class="card " style="width: 18rem;">
                    <img height="250px" style="object-fit: cover; object-position:center;" src="<%=user.picture %>" class="card-img-top" alt="...">
                    <div class="card-body">
                      <h5 class="card-title"><%= user.username %></h5>
                      <p class="card-text"><%= user.email %></p>

                      <h5>Likes: <%= user.likes.length %> <a href="/like/<%= user._id %>" class="btn m-3 btn-primary ">Like</a></h5>
                    </div>
                  </div>
                </div>
           <% })%>
          
        </div>
      </div>
   
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
  </body>
</html>